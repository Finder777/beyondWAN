{
  "routes": [
    {
      "route": "/protected/*",
      "allowedRoles": ["authenticated"],
      "postLoginRoutes": {
          "azureadb2c":"protected/home.html",
          "aad":"protected/home.html"
      }
    },
    {
      "route": "/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/*.html", "/css/*", "/js/*", "/media/*", "/images/*"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "Cache-Control": "no-cache, no-store, must-revalidate",
    "Pragma": "no-cache",
    "Expires": "0"
  }
}